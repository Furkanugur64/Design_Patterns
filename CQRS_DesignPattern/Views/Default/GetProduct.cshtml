@model CQRS_DesignPattern.CQRSPattern.Results.GetProductByIdQueryResult
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br /><br />

    <form method="post">
        <label>Ürün Id</label>
        <input type="text" asp-for="ProductID" class="form-control" />
        <br />
        <label>Ürün Adı</label>
        <input type="text" asp-for="ProductName" class="form-control" />
        <br />
        <label>Ürün Markası</label>
        <input type="text" asp-for="Brand" class="form-control" />
        <br />
    </form>
</div>